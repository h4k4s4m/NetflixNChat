{
  "manifest_version": 2,
  "name": "Netflix N' Chat",
  "version": "0.1",
  "description": "Make your binge watching a bit more social!",

  "content_scripts": [
    {
      "css": [
        "styles.css"
      ],
      "js": [
        "socket.js",
        "jquery.js",
        "content.js"
      ],
      "matches": [
        "https://www.netflix.com/*"
      ]
    }
  ]
}